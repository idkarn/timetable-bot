{
  "compilerOptions": {
    "module": "CommonJS",
    // "lib": ["ES2022"],
    "outDir": "./dist/",
    "strict": true,
    "rootDir": "./src",
    "target": "ES2021",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true
  }
}
